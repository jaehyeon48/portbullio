{
  "name": "portbullio-backend",
  "version": "1.0.0",
  "description": "A stock portfolio management service",
  "main": "src/index.ts",
  "scripts": {
    "start": "npm run dev",
    "dev": "SET NODE_ENV=development && nodemon -r tsconfig-paths/register ./src/index.ts",
    "build": "tsc -p .",
    "test": "jest --env=jsdom --watchAll",
    "format": "prettier --check ./src",
    "format:fix": "prettier --write ./src",
    "lint": "eslint ./src",
    "lint:fix": "eslint --fix ./src",
    "lint:staged": "lint-staged",
    "husky": "cd ../ && husky install ./backend/.husky"
  },
  "lint-staged": {
    "*.{js,ts,tsx,md}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "keywords": [
    "finance",
    "stock",
    "stock market",
    "stock portfolio",
    "investment"
  ],
  "author": "Jaehyeon Kim",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.25.0",
    "cookie-parser": "^1.4.6",
    "express": "^4.17.2",
    "winston": "^3.4.0",
    "winston-daily-rotate-file": "^4.6.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "dotenv": "^14.3.0",
    "eslint": "^8.7.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "jest": "^27.4.7",
    "lint-staged": "^12.3.1",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.5"
  }
}
