  "main": "src/index.ts",
  "scripts": {
    "start": "npm run dev",
    "dev": "SET NODE_ENV=development && nodemon -r tsconfig-paths/register ./src/index.ts",
    "prod": "NODE_ENV=production && nodemon -r tsconfig-paths/register ./src/index.ts",
    "build": "tsc -p .",
    "test": "jest --env=jsdom --watchAll",
    "format": "prettier --check ./src",
    "format:fix": "prettier --write ./src",
    "lint": "eslint ./src",
    "lint:fix": "eslint --fix ./src",
    "precommit": "npm run format:fix && npm run lint:fix"
