    "@types/node": "^17.0.18",
    "dotenv": "^16.0.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "nodemon": "^2.0.15",
    "ts-node": "^10.5.0",
    "tsconfig-paths": "^3.12.0"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity": "^3.51.0",
    "@aws-sdk/client-s3": "^3.51.0",
    "@aws-sdk/credential-provider-cognito-identity": "^3.51.0",
    "@prisma/client": "^3.9.2",
    "@types/spdy": "^3.4.5",
    "cookie-parser": "^1.4.6",
