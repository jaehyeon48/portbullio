{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "chrome": "51",
          "safari": "10.1",
          "ios": "10.3",
          "firefox": "54",
          "edge": "15"
        }
      }
    ],
    [
      "@babel/preset-react",
      {
        "runtime": "automatic"
      }
    ],
    "@babel/preset-typescript"
  ],
  "plugins": [
    [
      "@babel/plugin-transform-runtime",
      {
        "regenerator": true
      }
    ]
  ]
}
