{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@src/*": [
        "src/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@assets/*": [
        "src/assets/*"
      ],
      "@constants": [
        "src/constants/index.ts"
      ],
      "@styles/*": [
        "src/styles/*"
      ],
      "@hooks/*": [
        "src/hooks/*"
      ],
      "@types": [
        "src/types/index.ts"
      ],
      "@api/*": [
        "src/api/*"
      ],
      "@utils": [
        "src/utils/index.ts"
      ]
    }
  }
}
